@model IEnumerable<review.Models.category>

@{
    ViewBag.Title = "Index";
}
<html>
<head>
    <link href="@Url.Content("~/Content/css/cat.css")" rel="stylesheet" type="text/css" />
</head>
<body>
    <div class="wrapper">
        <h1>Categories</h1>


        <div class="clear"></div>
        <!-- items -->
        <div class="items">
            @foreach (var item in Model)
            {
            <div class="item">
                <img src=" @Url.Content(item.img) " width="250px" height="250px" alt="item" />
                <h2> @Html.ActionLink(item.Name, "Details", "subcategories", new { id = item.Id }, null)</h2>
            </div>
            }
            </div>
    </div>




    </body>
</html>